{
  "env": {
    "jquery": true,
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "_": false,
    "gl": false,
    "gon": false,
    "localStorage": false
  },
  "plugins": [
    "filenames"
  ],
  "rules": {
    "filenames/match-regex": [2, "^[a-z0-9_]+(.js)?$"]
  }
}
